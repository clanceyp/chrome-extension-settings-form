{
	"name": "Options Form"
	,"short_name": "set-f"
	,"version": "1.0.0"
	,"manifest_version": 2
	,"description": "Basic options form boiler plate"
	,"icons": {
		"32": "lib/options/i/cog-01.svg"
		,"48": "lib/options/i/cog.png"
		,"128": "lib/options/i/cog.png"
	}
	,"background":{
		"scripts": [
			"lib/options/js/manifest.js"
			,"lib/options/js/vendor/underscore.js"
			,"lib/options/js/vendor/zepto.js"
			,"lib/options/js/background.js"
		]
	}
	,"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
	,"options_page": "lib/options/index.html"
	,"permissions": [
		"tabs"
		,"http://*/*"
		,"https://*/*"
		,"background"
		,"notifications"
	]
}
